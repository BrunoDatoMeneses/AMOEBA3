<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.saveExplorer.SaveExplorer">
	<children>
		<Button mnemonicParsing="false" onAction="#handleRefresh"
			text="Refresh" />
		<HBox spacing="10.0">
			<children>
				<VBox alignment="CENTER">
					<children>
						<Label text="Auto saves" />
						<ComboBox fx:id="comboBoxA" />
						<HBox alignment="CENTER">
							<children>
								<Button mnemonicParsing="false" onAction="#handlePrevA">
									<graphic>
										<FontIcon iconLiteral="dashicons-controls-skipback"
											iconSize="32" />
									</graphic>
								</Button>
								<VBox alignment="CENTER">
									<children>
										<Button mnemonicParsing="false"
											onAction="#handleLaunchA" text="Launch as new" />
										<HBox>
											<children>
												<Button mnemonicParsing="false"
													onAction="#handleLoadA" text="Load" />
												<Button mnemonicParsing="false"
													onAction="#handlePreviewA" text="Preview" />
											</children>
										</HBox>
									</children>
								</VBox>
								<Button mnemonicParsing="false" onAction="#handleNextA">
									<graphic>
										<FontIcon iconLiteral="dashicons-controls-skipforward"
											iconSize="32" />
									</graphic>
								</Button>
							</children>
						</HBox>
					</children>
				</VBox>
				<VBox alignment="CENTER">
					<children>
						<Label text="Manual saves" />
						<ComboBox fx:id="comboBoxM" />
						<HBox alignment="CENTER">
							<children>
								<Button mnemonicParsing="false" onAction="#handlePrevM">
									<graphic>
										<FontIcon iconLiteral="dashicons-controls-skipback"
											iconSize="32" />
									</graphic>
								</Button>
								<VBox alignment="CENTER">
									<children>
										<Button mnemonicParsing="false"
											onAction="#handleLaunchM" text="Launch as new" />
										<HBox>
											<children>
												<Button mnemonicParsing="false"
													onAction="#handleLoadM" text="Load" />
												<Button mnemonicParsing="false"
													onAction="#handlePreviewM" text="Preview" />
											</children>
										</HBox>
									</children>
								</VBox>
								<Button mnemonicParsing="false" onAction="#handleNextM">
									<graphic>
										<FontIcon iconLiteral="dashicons-controls-skipforward"
											iconSize="32" />
									</graphic>
								</Button>
							</children>
						</HBox>
					</children>
				</VBox>
			</children>
		</HBox>
		<Label text="Preview :" />
	</children>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</VBox>
